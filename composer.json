{
    "name": "chiron/container",
    "type": "library",
    "description": "PSR-11 Container component",
    "keywords": ["psr11","psr-11","container","di","Dependency","Injection"],
    "homepage": "https://github.com/ncou/Chiron-Container",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "phpstan/phpstan": "^0.9.2"
    },
    "provide": {
        "psr/container-implementation": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Chiron\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": { "Chiron\\Tests\\" : "tests/" }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs",
            "@phpstan"
        ],
        "phpunit": "phpunit --colors=always",
        "phpstan": "phpstan analyse src --level=0",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
